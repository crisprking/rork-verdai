{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "buildConfiguration": "Release",
        "autoIncrement": true,
        "simulator": false,
        "enterpriseProvisioning": "universal",
        "credentialsSource": "remote"
      }
    },
    "production": {
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_FAST_RESOLVER": "1"
      },
      "ios": {
        "buildConfiguration": "Release",
        "autoIncrement": true,
        "simulator": false,
        "enterpriseProvisioning": "universal",
        "credentialsSource": "remote"
      }
    }
  },
  "submit": {
    "production": {}
  }
}